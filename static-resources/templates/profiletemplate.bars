            </div>
            <div class="col-md-2">
              <p class="lead text-center py-2">| More |</p>
            </div>
            <div class="col-md-2">
              <p class="lead text-center py-2"></p>
            </div>
            <div class="col-md-2">
              <p class="lead text-center py-2"></p>
            </div>
          </div>
          {{#each posts}}
          <div class="row bg-light mb-3">
